<%= form_for @user do |f|%>
<%= render 'shared/error_messages' %>
    <%= f.label :username %><br/>
    <%= f.text_field :username, placeholder:true  %><br/>
    <%= f.label :email %><br/>
    <%= f.email_field :email, placeholder:true %><br/>
    <%= f.label :password %><br/>
    <%= f.password_field :password %><br/><br/>
    <%= f.submit %>
<% end %>
 
